{
    "name": "studentos-backend",
    "version": "1.0.0",
    "description": "StudentOS Phase 1 API Server",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "migrate": "node dist/db/migrate.js",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "lint": "eslint src --ext .ts",
        "format": "prettier --write \"src/**/*.ts\""
    },
    "dependencies": {
        "@fastify/cors": "^9.0.1",
        "@fastify/env": "^5.0.1",
        "@supabase/supabase-js": "^2.39.3",
        "bullmq": "^5.1.10",
        "chrono-node": "^2.7.5",
        "dotenv": "^16.3.1",
        "fastify": "^4.25.2",
        "googleapis": "^131.0.0",
        "ioredis": "^5.3.2",
        "pino": "^8.17.2",
        "pino-pretty": "^10.3.1",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/node": "^20.10.6",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "eslint": "^8.56.0",
        "prettier": "^3.1.1",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3",
        "vitest": "^1.1.1"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}